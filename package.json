{
  "name": "react-node-concurrent",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm start --prefix backend\" \"npm start --prefix frontend\"",
    "install-all": "npm-run-all --parallel install-current install-frontend install-backend",
    "install-current": "npm install",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && npm install"
  },
  "dependencies": {
    "concurrently": "^9.1.0",
    "marked": "^15.0.3"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
